{
	"extends": "./tsconfig.options.json",
	"compilerOptions": {
		"target": "es6",
		"lib": ["dom", "es2016", "es2017.object"],
		"module": "esnext",
		"moduleResolution": "node",
		"jsx": "react",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"strict": true
	},
	"include": ["src/**/*.ts", "src/**/*.tsx"]
}
